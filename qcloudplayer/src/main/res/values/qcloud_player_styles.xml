<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SeekBarAppTheme" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/qcloud_player_progress_horizontal_holo_dark</item>
        <item name="android:indeterminateDrawable">@drawable/qcloud_player_progress_horizontal_holo_dark</item>
        <item name="android:minHeight">13dip</item>
        <item name="android:maxHeight">13dip</item>
        <item name="android:thumb">@drawable/qcloud_player_control_selector_holo_dark</item>
        <item name="android:thumbOffset">16dip</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
    </style>


    <style name="videoProgressBar">
        <item name="android:indeterminateDrawable">@anim/qcloud_player_video_loading_progressbar</item>
        <item name="android:minWidth">48dip</item>
        <item name="android:maxWidth">48dip</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:maxHeight">48dip</item>
        <item name="android:indeterminateOnly">true</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:indeterminateDuration">700</item>
    </style>
</resources>